[mysql]
host     = localhost

# логин и пароль для подключения к MySQL
username = user   
password = pass  

charset  = utf8

# опции по умолчанию
[options]         

# задержка между запросами к сайту, мс 
delay = 100 

# максимальная случайная задержка между запросами, мс
random = 100            

# файл со списком игнорируемых url-ов       
ignorefile = ignore.cnf   
